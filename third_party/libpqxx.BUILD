# The C++ connector for PostgreSQL.
# http://pqxx.org/development/libpqxx/

package(default_visibility = ["//visibility:public"])

cc_library(
    name = "libpqxx",
    srcs = glob([
        "lib/libpqxx*",
    ]),
    hdrs = glob([
        "include/pqxx/**/*",
    ]),
    includes = ["include"],
)
